{
  "assignees": [
    "kaidjohnson"
  ],
  "automerge": false,
  "branchPrefix": "renovate/",
  "enabledManagers": [
    "npm"
  ],
  "extends": [
    "config:base"
  ],
  "lockFileMaintenance": {
    "enabled": true,
    "pnpmWorkspaces": ["/"]
  },
  "packageRules": [
    {
      "groupName": "babel",
      "packageNames": [
        "babel-core"
      ],
      "packagePatterns": [
        "^@babel",
        "^babel-plugin"
      ]
    },
    {
      "groupName": "eslint",
      "packagePatterns": [
        "eslint"
      ]
    },
    {
      "groupName": "jest",
      "packagePatterns": [
        "jest"
      ]
    },
    {
      "groupName": "storybook",
      "packagePatterns": [
        "storybook"
      ]
    },
    {
      "groupName": "vue",
      "packagePatterns": [
        "vue"
      ]
    },
    {
      "groupName": "webpack",
      "packagePatterns": [
        "webpack",
        "-loader$"
      ]
    }
  ]
}
